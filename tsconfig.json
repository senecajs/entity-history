{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "commonjs",
    "sourceMap": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
